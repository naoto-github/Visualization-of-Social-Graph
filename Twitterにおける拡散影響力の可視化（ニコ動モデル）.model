//-------------------------------------------------------
// Agent Rule
//-------------------------------------------------------
#begin_rule UNIVERSE
Univ_Init{

	//コンソールのクリア
	ClearConsoleScreen()
	
	//nikodouモデルの初期位置
	initNikodo()
}

//nikodouの初期配置
sub initNikodo(){

	Dim width as double
	Dim height as double
	width = GetWidthSpace(Universe.Graph)
	height = GetHeightSpace(Universe.Graph)
	
	//対象ユーザ
	Dim target as Agt
	target = Universe.Graph.Node(Universe.Target)
	target.x_c = width / 2
	target.y_c = height / 2
	target.r = width / 2
	target.angle = PI() / 2
	//target.x = target.x_c + (0.7 * target.r) * cos(target.angle)
	target.x = width
	target.y = target.y_c + (0.7 * target.r) * sin(target.angle)
	
	//他のユーザ
	Dim node as Agt
	Dim nodes as AgtSet
	MakeAgtSet(nodes, Universe.Graph.Node)
	RemoveAgt(nodes, target)
	for each node in nodes
		node.x_c = width / 2 
		node.y_c = height / 2
		node.r = width / 2
		node.angle = Rnd() * 2 * PI();
		//node.x = node.x_c + (0.7 * node.r) * cos(node.angle)
		node.x = width
		node.y = node.y_c + (0.7 * node.r) * sin(node.angle)
	next node
}

Univ_Step_Begin{

}

Univ_Step_End{

}

Univ_Finish{

}
#end_rule UNIVERSE




//-------------------------------------------------------
// Agent Rule
//-------------------------------------------------------
#begin_rule UNIVERSE.GRAPH.NODE
Agt_Init{

	//透明度
	My.Alpha = 100

	//ツイート番号
	My.TweetNumber = 0

	//キーワード情報の読込
	readKeywords()
	
	//ツイート情報の読込
	readTweets()

	//初期ツイート
	if CountAgtSet(My.Tweets) > 0 then
		My.CurrentTweet = GetAgt(My.Tweets, My.TweetNumber)
	end if

	//ターゲットの設定
	Dim tweet as Agt
	if My.ID == Universe.Target then
		My.Alpha = 0
		tweet = GetAgt(My.Tweets, 0)
		Universe.BaseTime = tweet.time
	end if
	
	//スピードの設定
	My.Speed = Rnd() * (Universe.MaxSpeed - 1) + 1
	
}

Agt_Step{

	//ツイートの表示
	showTweet()
	
	//ツイートの非表示
	//hideTweet()
	
	//左へ動く
	if CountAgtSet(My.Tweets) > 0 then
		moveLeft()
	end if
}

//左へ動く
sub moveLeft(){
	My.X = My.X - My.Speed
	My.CurrentTweet.X = My.CurrentTweet.X - My.Speed
}

//ツイートの表示
sub showTweet(){
	Dim width as double
	Dim tweet as Agt
	Dim i as integer
	Dim size as integer
	Dim counter as integer
	Dim hand as Agt
	width = GetWidthSpace(Universe.Graph)
	size = CountAgtSet(My.Tweets)
	counter = GetCountStep()
	hand = Universe.Graph.MinuteHand(0)
	if size > 0 then
		for i=My.TweetNumber to size-1
			tweet = GetAgt(My.Tweets, i)
			if (tweet.time >= Universe.BaseTime) and
			   (tweet.time < Universe.BaseTime + counter * Universe.Unit) then	   
			   My.CurrentTweet = tweet
			   //My.X = My.X_C + (0.7 * My.R) * cos(DegreeToRad(hand.direction) + (PI() / 2))
			   My.X = width + 50
			   My.Y = My.Y_C + (0.7 * My.R) * sin(DegreeToRad(hand.direction) + (PI() / 2))
			   My.CurrentTweet.X = My.X
			   My.CurrentTweet.Y = My.Y - 40
				My.Alpha = 0
				My.TweetNumber = i + 1
				break
			end if
		next i
	end if
	
}

//ツイートの非表示
sub hideTweet(){
	if My.Alpha < 100 then
		My.Alpha = My.Alpha + 1
	else
		My.Alpha = 100
	end if
		
	if My.Alpha == 100 then
		if CountAgtSet(My.Tweets) > 0 then
			My.CurrentTweet.X = -9999
			My.CurrentTweet.Y = -9999
		end if
	end if
	
}

//ツイート情報の読込
sub readTweets(){

	//ファイルを開く
	Dim line as String
	Dim tweet as Agt
	Dim tmp1 as String
	Dim tmp2 as String
	if OpenFile(My.TweetFile, My.ID, 1) then
		Do While true	
			if IsEofFile(My.ID) then
				CloseFile(My.ID)
				break;
			else
				line = ReadFile(My.ID)
				tweet = CreateAgt(Universe.Graph.Tweet)
				//tweet.time = GetToken(line, 2)
				tweet.text = Left(GetToken(line, 3), Universe.TextLength)
				tmp1 = GetToken(line, 2)
				tmp2 = Left(tmp1, Len(tmp1)-1)
				tweet.time = CLong(Right(tmp2, Len(tmp2)-1))
				tweet.time_s = TimeToStr(tweet.time, "yyyy/MM/dd HH:mm:ss")
				
				//println("String:" & tweet.time)
				//println("Long:" & TimeToStr(tweet.time, "yyyy/MM/dd HH:mm:ss"))
				
				AddAgt(My.tweets, tweet)
			end if
		Loop
	end if

}

//キーワード情報の読込
sub readKeywords(){

	//ファイルを開く
	Dim line as String
	Dim keyword as Agt
	Dim tmp1 as String
	Dim tmp2 as String
	if OpenFile(My.KeywordFile, My.ID, 1) then
		Do While true	
			if IsEofFile(My.ID) then
				CloseFile(My.ID)
				break;
			else
				line = ReadFile(My.ID)
				keyword = CreateAgt(Universe.Graph.Keyword)
				tmp1 = GetToken(line, 0)
				keyword.value = GetToken(line, 4)
				tmp2 = Left(tmp1, Len(tmp1)-1)
				keyword.token = Right(tmp2, Len(tmp2)-1)
				
				AddAgt(My.Keywords, keyword)
			end if
		Loop
	end if
}
#end_rule UNIVERSE.GRAPH.NODE




