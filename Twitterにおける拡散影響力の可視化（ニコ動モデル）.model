#begin_rule UNIVERSE.GRAPH.HOURHAND
Agt_Init{
	Dim width as Integer
	Dim height as Integer
	
	width = GetWidthSpace(Universe.Graph)
	height = GetHeightSpace(Universe.Graph)
	
	My.X = (width / 2)
	My.Y = (height / 2)
	
	My.Image = "時針.png"
	
	My.Diff = 360 / 720
	//My.Diff = 360 / 120
}

Agt_Step{
	if My.Diff < 360 then
		My.Direction = My.Direction - My.Diff
	else
		My.Direction = 0
	end if
}
#end_rule UNIVERSE.GRAPH.HOURHAND




//-------------------------------------------------------
// Agent Rule
//-------------------------------------------------------
#begin_rule UNIVERSE.GRAPH.MINUTEHAND
Agt_Init{
	Dim width as Integer
	Dim height as Integer
	
	width = GetWidthSpace(Universe.Graph)
	height = GetHeightSpace(Universe.Graph)
	
	My.X = (width )
	My.Y = (height)
	My.Direction = 180
	My.Diff = 360 / 60
	//My.Diff = 360 / 10
}

Agt_Step{
	if My.Diff < 360 then
		My.Direction = My.Direction - My.Diff
		Forward(100)
	else
		My.Direction = 0
	end if
}
#end_rule UNIVERSE.GRAPH.MINUTEHAND




